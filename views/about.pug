extends layout

block content
  div.content.aboutpage
    navbar
      nav.navbar.navbar-expand-sm.navbar-light.bg-white#main-nav
        .container-fluid
          a.navbar-brand(href="/home") Breeze
          button.navbar-toggler(data-toggle="collapse" data-target="#navbarCollapse")
            span.navbar-toggler-icon
          .collapse.navbar-collapse#navbarCollapse
            ul.navbar-nav.ml-auto
              li.nav-item.active
                a.nav-link(href="/home") Home
              li.nav-item
                a.nav-link(href="/about") About
              li.nav-item.dropdown
                a.nav-link.dropdown-toggle(href="#" data-toggle="dropdown")
                  span Food
                .dropdown-menu
                  a.dropdown-item(href="/spring") SPRING  
                  a.dropdown-item(href="/summer") SUMMER   
                  a.dropdown-item(href="/autumn") Autumn  
                  a.dropdown-item(href="/winter") Winter       
              li.nav-item
                a.nav-link(href="/contact") Contact
    header#page-header
      .dark-overlay
        .container-fluid
          .row
            .col-md-6.m-auto.text-center.headerinfo
              div.headerinfo-contents
                h1 FOOD STYLIST
                h2.text-primary Oh sukyung
                span Lorem ipsum dolor, sit amet consectetur adipisicing elit. Et, inventore!
                input#headerimagepath(type="hidden" value= aboutimages[0].headerimage) 
            